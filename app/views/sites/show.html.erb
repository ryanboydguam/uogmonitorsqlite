<table class="centerShow">
      <tr>
        <th >Delay</th>
        <th >Local Time</th>
      </tr>
      <%data_holder = @networkpages.pluck(:relaytime,:creation).reverse()%>
      <%@networkpages.each do |data|%>
        <%if data.relaytime >= 5%>
          <tr style="color:red;">
            <td>OFFLINE</td>
            <td><%=l(data.creation,locale: :en)%></td>
          </tr>
        <%else%>
        <tr style="color:white;">
            <td><%=data.relaytime%></td>
            <td><%=l(data.creation,locale: :en)%></td>
          </tr>
        <%end%>
      <%end%>
    </table>


